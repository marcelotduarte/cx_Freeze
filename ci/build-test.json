      "PyQt6-Qt6 --mingw=",
      "PyQt6-WebEngine --mingw=",
      "PyQt6-WebEngine-Qt6 --mingw="
    ],
    "platform": ["!mingw", "!conda"]
  },
  "pyside2": {
    "test_app": "gui:test_pyside2",
    "requirements": "PySide2",
    "platform": ["!macos:arm64"],
    "python-versions": ["3.8", "3.9", "3.10"],
    "comment": "pyside2 does not support macos arm64"
  },
  "pyside2-simplebrowser": {
    "test_app": "gui:test_simplebrowser",
    "requirements": ["PySide2", "--conda=qt-webengine"],
    "platform": ["!macos:arm64", "!mingw"],
    "python-versions": ["3.8", "3.9", "3.10"]
  },
  "pyside6": {
    "test_app": "gui:test_pyside6",
    "requirements": "PySide6"
  },
