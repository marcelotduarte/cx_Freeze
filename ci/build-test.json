  },
  "pyqt5": {
    "test_app": "gui:test_pyqt5",
    "requirements": "PyQt5 --conda=pyqt"
  },
  "pyqt5-simplebrowser": {
    "test_app": ["gui:test_simplebrowser"],
    "requirements": [
      "PyQt5 --conda=pyqt",
      "PyQtWebEngine --conda=pyqtwebengine"
    ],
    "platform": ["!mingw"]
  },
  "pyqt6": {
    "test_app": "gui:test_pyqt6",
    "requirements": ["PyQt6", "PyQt6-Qt6 --mingw="],
    "platform": ["!conda"],
    "comment": "conda doesn't support pyqt6"
  },
  "pyqt6-qml": {
    "test_app": "gui:test_qml",
    "requirements": ["PyQt6", "PyQt6-Qt6 --mingw="],
    "platform": ["!conda"]
