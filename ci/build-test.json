{
  "manifest": {
    "test_app": ["test_manifest", "test_simple_manifest"],
    "platform": ["windows", "mingw"]
  },
  "matplotlib": {
    "test_app": "gui:matplotlib_eg",
    "find_links": [
      "https://wxpython.org/Phoenix/snapshot-builds/",
      "https://extras.wxpython.org/wxPython4/extras/linux/gtk3/ubuntu-18.04/",
      "https://extras.wxpython.org/wxPython4/extras/linux/gtk3/ubuntu-20.04/"
    ],
    "requirements": ["numpy", "pillow --no-deps", "matplotlib", "wxPython"]
  },
  "matplotlib_tk": {
    "test_app": "gui:test_matplotlib_tk",
    "requirements": ["numpy", "pillow --no-deps", "matplotlib"]
  },
  "opencv": {
    "test_app": "gui:test_opencv",
    "requirements": [
      "opencv-python --mingw=python-opencv --conda=py-opencv",
      "opencv-contrib-python --platform=macos --conda=opencv",
      "vtk"
    ]
  },
  "orjson": {
    "test_app": "test_orjson",
    "requirements": "orjson",
    "platform": ["!mingw"]
  },
  "pandas": {
    "test_app": "test_pandas",
    "requirements": ["numpy", "pandas"]
  },
  "pillow": {
    "test_app": "test_pillow",
    "requirements": "pillow --no-deps"
  },
  "pycountry": {
    "test_app": "test",
